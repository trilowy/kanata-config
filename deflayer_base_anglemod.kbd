;; angle-mod: the ISO key (a.k.a. LSGT or 102 key) becomes Z

(deflayer base
  tab       q    w    e    r    t    y    u    i    o    p    [    ]    \
  caps      a    s    d    f    g    h    j    k    l    ;    '    ret
  lsft z    x    c    v    b    bspc n    m    ,    .    /    rsft
  lctl      lmet lalt           spc            ralt rmet
)

